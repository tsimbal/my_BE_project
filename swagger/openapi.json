  "components": {
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    },
    "schemas": {
      "Auth": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        },
        "required": ["email", "password"]
      },
      "ResLogin": {
        "type": "object",
        "properties": {
          "access_token": {
            "type": "string",
            "default": "ey212sfagojerta"
          }
        }
      },
      "User": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "default": 200
          },
          "data": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "surname": {
                "type": "string"
              },
              "sex": {
                "type": "number"
              },
              "email": {
                "type": "string"
              },
              "password": {
                "type": "string"
              },
              "zip": {
                "type": "string"
              },
              "phone": {
                "type": "string"
              },
              "birthday": {
                "type": "string"
              },
              "city": {
                "type": "string"
              },
              "country": {
                "type": "string"
              },
              "address": {
                "type": "string"
              },
              "updatedAt": {
                "type": "string"
              },
              "createdAt": {
                "type": "string"
              }
            },
            "required": ["email"]
          }
        }
      },
      "Task": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          }
        },
        "required": ["title"]
      },
      "ResCreateTask": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "default": 200
          },
          "data": {
            "type": "object",
            "properties": {
              "title": {
                "type": "string"
              },
              "comleted": {
                "type": "boolean",
                "default": false
              },
              "comletedDate": {
                "type": "string",
                "default": "132154668894"
              },
              "owner": {
                "type": "string"
              },
              "_id": {
                "type": "string"
              }
            }
          }
        }
      },
      "ResGetTasks": {
        "type": "object",
        "properties": {
          "statusCode": {
            "type": "number",
            "default": 200
          },
          "data": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "title": {
                  "type": "string"
                },
                "comleted": {
                  "type": "boolean",
                  "default": false
                },
                "comletedDate": {
                  "type": "string",
                  "default": "132154668894"
                },
                "owner": {
                  "type": "number"
                },
                "_id": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  }
